{
	"name": "brave-frontend",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": "18.12.1"
	},
	"scripts": {
		"copy:bundle": "cp ./bundle.js ./node_modules/reshaped/bundle.js",
		"dev": "npm run copy:bundle && next dev",
		"start": "npm run copy:bundle && next start",
		"build": "npm run copy:bundle && next build",
		"lint": "next lint",
		"build:themes": "reshaped theming --output src/themes"
	},
	"dependencies": {
		"@editorjs/attaches": "^1.3.0",
		"@editorjs/delimiter": "^1.3.0",
		"@editorjs/editorjs": "^2.27.2",
		"@editorjs/header": "^2.7.0",
		"@editorjs/image": "^2.8.1",
		"@editorjs/link": "^2.5.0",
		"@editorjs/list": "^1.8.0",
		"@editorjs/table": "^2.2.2",
		"@editorjs/underline": "^1.1.0",
		"@reduxjs/toolkit": "^1.9.5",
		"chart.js": "^4.3.3",
		"chartjs-plugin-annotation": "^3.0.1",
		"chartjs-plugin-datalabels": "^2.2.0",
		"date-fns": "^2.30.0",
		"html-to-react": "^1.6.0",
		"next": "12.3.4",
		"next-redux-wrapper": "^8.1.0",
		"react": "18.2.0",
		"react-datepicker": "^4.12.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "18.2.0",
		"react-hook-form": "^7.43.9",
		"react-input-mask": "^3.0.0-alpha.2",
		"react-redux": "^8.0.5",
		"reset-css": "^5.0.1",
		"reshaped": "./vendor/reshaped.tgz",
		"styled-components": "^5.3.10",
		"tributejs": "^5.1.3",
		"typescript": "5.0.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.3",
		"@commitlint/config-conventional": "^17.6.3",
		"@types/chart.js": "^2.9.37",
		"@types/editorjs__header": "^2.6.0",
		"@types/node": "^18.16.10",
		"@types/react": "18.2.6",
		"@types/react-datepicker": "^4.11.2",
		"@types/react-dnd-scrollzone": "^5.0.0",
		"@types/react-dom": "18.2.4",
		"@types/react-input-mask": "^3.0.2",
		"@types/styled-components": "^5.1.26",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"babel-plugin-styled-components": "^2.1.1",
		"commitizen": "^4.3.0",
		"cz-customizable": "^7.0.0",
		"editorjs-hyperlink": "^1.0.6",
		"eslint": "^8.40.0",
		"eslint-config-next": "13.4.2",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"tributejs": "^5.1.3",
		"typescript": "5.0.4"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "./commitizen.js"
		}
	},
	"lint-staged": {
		"*.{js,ts,tsx,jsx}": [
			"prettier --write",
			"node_modules/.bin/eslint --max-warnings=0"
		]
	}
}
